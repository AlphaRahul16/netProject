        
function ResizeMenu()
{
    var w = 150;
    var h = 20;
    if (navigator.userAgent.toLowerCase().indexOf("msie") > 0)
    {
        // if IE
        w = document.forms[0].scrollWidth;
        h = document.forms[0].scrollHeight;
        if (w < 150) { w = 150; }   // set a minimum width for the menu
        //if (h < 20) { h = 20; }     // set a minimum height for the menu
        try { window.resizeTo(w, h); } catch(ex) { }
    }
    else if (window.frameElement != null)
    {
        // else (FireFox, Netscape)
        w = document.forms[0].offsetWidth;
        h = document.forms[0].offsetHeight;
        if (w < 150) { w = 150; }   // set a minimum width for the menu
        //if (h < 20) { h = 20; }     // set a minimum height for the menu
        window.frameElement.style.width = w + "px";
        window.frameElement.style.height = h + "px";
    }
    if (parent != null && window.frameElement != null)
	{
	    var t = GetTop(window.frameElement);
	    var l = GetLeft(window.frameElement);
	    CheckOpenToLeftOrTop(t, l, w, h);
        // create a shadow box for the newly resized and repositioned menu
       // if (parent.CreateShadowBox) { parent.CreateShadowBox(window.frameElement.id, GetLeft(window.frameElement), GetTop(window.frameElement), w, h); }
    }
}

function CheckOpenToLeftOrTop(t, l, w, h)
{
    var bNeedsToMove = false;
    
    var moveToLeft = GetRequest("MoveToLeft");
    if (moveToLeft == "1")
	{
	    var parentId = GetRequest("ParentId");
	    var widthAdj = GetRequest("WidthAdj");
	    var heightAdj = GetRequest("HeightAdj");
	    if (parentId != "")
	    {
	        // find the parent link (which opened this menu) and position the window frame to the left of it
	        var parentLink;
	        if (window.frameElement.document != null)
	            parentLink = window.frameElement.document.getElementById(parentId);
	        else
	            parentLink = window.frameElement.ownerDocument.getElementById(parentId);
		    if (parentLink != null)
		    {
		        if (widthAdj == "") { widthAdj = "0"; }
		        try { widthAdj = parseInt(widthAdj); } catch(ex) { widthAdj = 0; }
			    var l = GetLeft(parentLink) - w + parentLink.offsetWidth + widthAdj;
			    bNeedsToMove = true;
		    }
		}
	}
	
	var moveToTop = GetRequest("MoveToTop");
	if (moveToTop == "1")
	{
	    var parentId = GetRequest("ParentId");
	    var widthAdj = GetRequest("WidthAdj");
	    var heightAdj = GetRequest("HeightAdj");
	    if (parentId != "")
	    {
	        // find the parent link (which opened this menu) and position the window frame to the left of it
		    var parentLink;
		    if (window.frameElement.document != null)
		        parentLink = window.frameElement.document.getElementById(parentId);
		    else
		        parentLink = window.frameElement.ownerDocument.getElementById(parentId);
		    if (parentLink != null)
		    {
		        if (heightAdj == "") { heightAdj = "0"; }
		        try { heightAdj = parseInt(heightAdj); } catch(ex) { heightAdj = 0; }
			    var t = GetTop(parentLink) - h + heightAdj;
			    bNeedsToMove = true;
		    }
		}
	}
	
	if (bNeedsToMove)
	{
	    if (l < 0) { l = 0; }   // set a minimum left for the menu
        if (t < 0) { t = 0; }   // set a minimum top for the menu
	    if (navigator.userAgent.toLowerCase().indexOf("msie") > 0)
        {
            // if IE
	        try { window.moveTo(l, t); } catch(ex) { }
	    }
	    else if (window.frameElement != null)
        {
            // else (FireFox, Netscape)
            window.frameElement.style.left = l +"px";
            window.frameElement.style.top = t + "px";
        }
	}
}

// functions to keep the menus open:

function ShowMenu()
{
	if (parent != null && parent.ShowContextMenu) { parent.ShowContextMenu(null, null, null, null, 0, 0, null, null, false); }
}

function HideMenu()
{
	if (parent != null && parent.HideContextMenu) { parent.HideContextMenu(null); }
}

function GetRequest(name)
{
    var v = "";
    var s = document.location.href;
    if (s.toLowerCase().indexOf(name.toLowerCase() + "=") >= 0)
    {
        v = s.substring(s.toLowerCase().indexOf(name.toLowerCase() + "=") + name.length + 1);
        if (v.indexOf("&") >= 0) { v = v.substring(0, v.indexOf("&")); }
    }
    return v;
}

// ----- vertical sliding (overflow) menu:

var nOverflowMenuOpenTimerId = null;
var nOverflowMenuCloseTimerId = null;
var sLastOpenOverflowMenuId = "";
var oLastOpenOverflowMenuParentLink = null;
var sLastOpenOverflowMenuParentLinkCssClass = "";

function SlideMenu(id, resize, link, cssClass, activeCssClass)
{
	var e = FindControl(id);
	if (e != null)
	{
		if (e.style.display == "none")
		{
		    if (link != null && activeCssClass != null && activeCssClass != "")
		    {
		        var currentCssClass = link.className;
		        link.className = activeCssClass;
		        if (oLastOpenOverflowMenuParentLink != null)
		        {
		            if (sLastOpenOverflowMenuParentLinkCssClass != "") { oLastOpenOverflowMenuParentLink.className = sLastOpenOverflowMenuParentLinkCssClass; }
		            else if (cssClass != null && cssClass != "") { oLastOpenOverflowMenuParentLink.className = cssClass; }
		        }
		        sLastOpenOverflowMenuParentLinkCssClass = currentCssClass;
		        oLastOpenOverflowMenuParentLink = link;
		    }
			e.style.height = "0px";
			e.style.display = "inline";
			CollapseSlideMenu(sLastOpenOverflowMenuId, resize);
			ExpandSlideMenu(id, resize);
			sLastOpenOverflowMenuId = id;
		}
		else if (e.style.display == "inline")
		{
	        if (link != null && cssClass != null && cssClass != "") { link.className = cssClass; }
	        oLastOpenOverflowMenuParentLink = null;
	        sLastOpenOverflowMenuParentLinkCssClass = "";
			CollapseSlideMenu(id, resize);
			sLastOpenOverflowMenuId = "";
		}
	}
}

function ExpandSlideMenu(id, resize)
{
	var e = FindControl(id);
	var o = FindControl(id + "_child");
	if (e != null && o != null)
	{
	    if (parseInt(e.style.width) < o.offsetWidth) { e.style.width = o.offsetWidth; }
	    //else { e.style.width = "100%"; }
		var h = parseInt(e.style.height);
		if (h < o.scrollHeight)
		{   
		    if (h + 50 > o.scrollHeight) { h = o.scrollHeight - 50; }
			o.style.clip = "rect(0," + o.offsetWidth + "," + (h + 50) + ",0)";
			e.style.height = (h + 50) + "px";
			nOverflowMenuOpenTimerId = setTimeout("ExpandSlideMenu('" + id + "'," + (resize ? "true" : "false") + ")", 10);
		}
		else
		{
			clearTimeout(nOverflowMenuOpenTimerId);
			nOverflowMenuOpenTimerId = null;
		}
		if (resize && window.ResizeMenu) { ResizeMenu(); }
		else if (sLastContextMenuId != null && sLastContextMenuId != "")
		{
		    var menu = FindControl(sLastContextMenuId);
		   // if (menu != null) { CreateShadowBox(sLastContextMenuId, GetLeft(menu), GetTop(menu), menu.offsetWidth, menu.offsetHeight); }
		}
	}
}

function CollapseSlideMenu(id, resize)
{
	var e = FindControl(id);
	var o = FindControl(id + "_child");
	if (e != null && o != null)
	{
		if (parseInt(e.style.width) < o.offsetWidth) { e.style.width = o.offsetWidth; }
		//else { e.style.width = "100%"; }
		var h = parseInt(e.style.height);
		if (h > 0)
		{
		    if (h < 50) { h = 50; }
			o.style.clip = "rect(0," + o.offsetWidth + "," + (h - 50) + ",0)";
			e.style.height = (h - 50) + "px";
			nOverflowMenuCloseTimerId = setTimeout("CollapseSlideMenu('" + id + "'," + (resize ? "true" : "false")  + ")", 10);
		}
		else
		{
			clearTimeout(nOverflowMenuCloseTimerId);
			nOverflowMenuCloseTimerId = null;
			e.style.display = "none";
		}
		if (resize && window.ResizeMenu) { ResizeMenu(); }
		else if (sLastContextMenuId != null && sLastContextMenuId != "")
		{
		    var menu = FindControl(sLastContextMenuId);
		  //  if (menu != null) { CreateShadowBox(sLastContextMenuId, GetLeft(menu), GetTop(menu), menu.offsetWidth, menu.offsetHeight); }
		}
	}
}

