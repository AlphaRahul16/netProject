}output+='<a id="groupId" href="'+Manager.createLink('g:"'+doc.g+'"',"ga",null)+'" onclick="return Manager.createHistoryAndLoad(true,\'g:&quot;'+doc.g+"&quot;','ga',null);\">"+doc.g+"</a></td>";
