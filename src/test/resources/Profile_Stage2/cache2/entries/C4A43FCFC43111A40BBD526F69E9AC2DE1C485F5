var query=splits[2];
if(!Util.isEmpty(query)){$("#query").val(query);
PageManager.showPage($("#searchResultPage"));
this.doQuery(query,this.currentType,page)
}}},createLink:function(query,type,page){if(!query){query=$("#query").val()
}if(!page){page=1
}if(!type){type=this.currentType;
if(!type){type="ga"
}}return"#"+encodeURIComponent("search|"+type+"|"+page+"|"+query)
},createHistoryAndLoad:function(reset,query,type,page){if(reset){$("#query").val("");
this.currentType="ga"
}if(query){$("#query").val(query)
}if(!page){page=1
}if(!type){type=this.currentType;
if(!type){type="ga"
}}this.currentType=type;
$.history.load("search|"+type+"|"+page+"|"+$("#query").val(),true);
return false
},createAllHistoryAndLoad:function(groupId,artifactId){return this.createHistoryAndLoad(true,'g:"'+groupId+'" AND a:"'+artifactId+'"',"gav")
},createAllLink:function(groupId,artifactId){return this.createLink('g:"'+groupId+'" AND a:"'+artifactId+'"',"gav")
},doQuery:function(query,type,page){this.store.addByValue("q",query);
this.store.addByValue("rows",20);
if(type=="gav"){this.store.addByValue("core",type)
}else{this.store.remove("core")
}if(page){this.store.addByValue("start",(page-1)*20)
}else{this.store.remove("start")
}_gaq.push(["_trackEvent",type,"search",query,page]);
this.doRequest()
},registerListeners:function(){$("#query").keyup(function(e){if(e.keyCode==13){Manager.createHistoryAndLoad(true,$("#query").val())
}});
$("#queryButton").click(function(){Manager.createHistoryAndLoad(true,$("#query").val())
})
},showResultPage:function(){PageManager.showPage($("#searchResultPage"))
}});
Manager.resultWidget=new AjaxSolr.ResultWidget({id:"result",target:"#docs"});
Manager.pagerWidget=new AjaxSolr.CentralPagerWidget({id:"pager",target:"#pager",prevLabel:"&lt;",nextLabel:"&gt;",innerWindow:1,renderHeader:function(perPage,offset,total){$("#pager-header").html($("<span/>").text("displaying "+Math.min(total,offset+1)+" to "+Math.min(total,offset+perPage)+" of "+total))
}});
Manager.footerPagerWidget=new AjaxSolr.CentralPagerWidget({id:"footerPager",target:"#footerPager",prevLabel:"&lt;",nextLabel:"&gt;",innerWindow:1,renderHeader:function(perPage,offset,total){$("#footerPager-header").html($("<span/>").text("displaying "+Math.min(total,offset+1)+" to "+Math.min(total,offset+perPage)+" of "+total))
}});
Manager.addWidget(Manager.resultWidget);
Manager.addWidget(Manager.pagerWidget);
Manager.addWidget(Manager.footerPagerWidget);
Manager.init();
Manager.registerListeners();
$("#query").focus()
})
})(jQuery);
