}}return -1
}function guid(prefix){return(prefix||"")+Math.round(Math.random()*1000000).toString()
}function merge(obj1,obj2){var result={},name;
for(name in obj1){result[name]=obj1[name]
}for(name in obj2){result[name]=obj2[name]
}return result
}function toBoolean(value){var result={"true":true,"false":false}[value];
return result==null?value:result
}function popup(url,name,width,height,options){var x=(screen.width-width)/2,y=(screen.height-height)/2;
