}if(!Util.isEmpty(a)){query+=(query=="")?'a:"'+a+'"':' AND a:"'+a+'"'
}if(!Util.isEmpty(v)){query+=(query=="")?'v:"'+v+'"':' AND v:"'+v+'"'
}if(!Util.isEmpty(l)){query+=(query=="")?'l:"'+l+'"':' AND l:"'+l+'"'
