e.preventDefault=function(){this.returnValue=false
}
}func.call(scope||window,e)
}if(obj.attachEvent){obj.attachEvent("on"+type,handler)
}else{obj.addEventListener(type,handler,false)
}}function alert(str){window.alert(sh.config.strings.alert+str)
}function findBrush(alias,showAlert){var brushes=sh.vars.discoveredBrushes,result=null;
if(brushes==null){brushes={};
