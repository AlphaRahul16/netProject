}else{Manager.createHistoryAndLoad(true,this.query)
}return false
},createLink:function(){return"#"+encodeURIComponent("advancedsearch|gav")
},createClassnameHistoryAndLoad:function(reset,classname,skipAdvancedSearchHistory){if(reset){$("#classname").val("")
}var doQuery=false;
