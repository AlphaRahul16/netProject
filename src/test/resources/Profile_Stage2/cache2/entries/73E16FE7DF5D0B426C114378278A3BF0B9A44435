<script type="text/javascript"> 
var i = 0; 
var image = new Array();   
// LIST OF IMAGES 
image[0] = "images/Website-Banner-Chizza-Mania.png";///changed on 1st Nov images/Leaflet-KFCwebsite-banner.png
image[1] = "iimages/Website-Banner-Chizza-Mania.png";
//image[2] = "images/offer_banner_3.jpg"; 
var k = image.length-1;    

var link= new Array();   
// LIST OF LINKS 
link[0] = "http://www.chizzamillionmania.com/?utm_source=kfcsite&utm_medium=cpc&utm_campaign=siteconfi&utm_content=confidential";
link[1] = "http://www.chizzamillionmania.com/?utm_source=kfcsite&utm_medium=cpc&utm_campaign=siteconfi&utm_content=confidential";
//link[2] = "https://delivery.kfc.co.in/kfcindia/web?utm_source=kfcsite&utm_medium=topbanner&utm_campaign=wingl";

function swapImage(){  
var img = document.getElementById("slide"); 
img.src= image[i]; 
var a = document.getElementById("link"); 
a.href= link[i]; 

if(i < k ) { i++;}  
else  { i = 0; } 
setTimeout("swapImage()",6000); 
} 
function addLoadEvent(func) { 
var oldonload = window.onload; 
if (typeof window.onload != 'function') { 
window.onload = func; 
} else  { 
window.onload = function() { 
if (oldonload) { 
oldonload(); 
} 
func(); 
} 
} 
} 
addLoadEvent(function() { 
swapImage(); 
});  
</script> 
