	<test name="COE Inventory Smoke Test" group-by-instances="true">
		<classes>
			<class name="com.qait.tests.COE_Inventory_Test"></class>
		</classes>
	</test>
	<test name="ACS Chapter Relationships Test" group-by-instances="true">
		<classes>
			<class name="com.qait.tests.ACS_Chapter_Relationships_Test"></class>
		</classes>
	</test>

	<test name="Inventory Product Fulfillment Test"
		group-by-instances="true">
		<classes>
			<class name="com.qait.tests.Inventory_Product_Fulfillment_Test" />
		</classes>
	</test>
	<test name="ACS PBA Test" group-by-instances="true">
		<classes>
			<class name="com.qait.tests.ACS_PBA_Test" />
		</classes>
	</test>
	<test name="ACS Void Invoice Test" group-by-instances="true">
		<classes>
			<class name="com.qait.tests.ACS_Void_Invoice_Test" />
