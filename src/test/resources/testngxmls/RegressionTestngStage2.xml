	</test>
	<test name="ACS Rubber Insurance Test" group-by-instances="true">
		<classes>
			<class name="com.qait.tests.ACS_Rubber_Insurance_Test"></class>
		</classes>
	</test>
	<test name="ACS AACT OMR Test" group-by-instances="true">
		<classes>
			<class name="com.qait.tests.ACS_AACT_OMR"></class>
		</classes>
	</test>

	<test name="ACS MGM Test" group-by-instances="true">
		<classes>
			<class name="com.qait.tests.ACS_MGM_Test"></class>
		</classes>
	</test>
	<test name="Subscription Fulfillment" parallel="false">
		<classes>
			<class name="com.qait.tests.Subscription_Fulfillment_Test"></class>
		</classes>
