<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="ACS Tests" preserve-order="true" thread-count="1"
	parallel="tests">
	<test name="COE Inventory Smoke Test" group-by-instances="true">
		<classes>
			<class name="com.qait.tests.COE_Inventory_Test"></class>
		</classes>
	</test>
	<test name="Inventory Product Fulfillment Test"
		group-by-instances="true" thread-count="1" parallel="instances">
		<classes>
			<class name="com.qait.tests.Inventory_Product_Fulfillment_Test" />
		</classes>
	</test>
	<test name="ACS_CCED_NCW_MemberNumberLookup_Test"
		group-by-instances="true">
		<classes>
			<class name="com.qait.tests.ACS_CCED_NCW_MemberNumberLookup_Test"></class>
		</classes>
	</test>
	<test name="Subscription Fulfillment" parallel="false">
		<classes>
			<class name="com.qait.tests.Subscription_Fulfillment_Test"></class>
		</classes>
	</test>
	<test name="ACS MGM Test" group-by-instances="true">
		<classes>
			<class name="com.qait.tests.ACS_MGM_Test"></class>
		</classes>
	</test>

</suite>