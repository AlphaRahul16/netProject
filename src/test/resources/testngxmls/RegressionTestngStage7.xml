<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="ACS Tests" preserve-order="true" thread-count="1"
	configfailurepolicy="continue" parallel="tests">

	<test name="ACS_AddressChangeTermsInvoice_Test"
