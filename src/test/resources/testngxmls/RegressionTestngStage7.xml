<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="ACS Tests" preserve-order="true" thread-count="2"
	parallel="tests">

	<test name="ACS_AddressChangeTermsInvoice_Test"
		group-by-instances="true">
		<classes>
			<class name="com.qait.tests.ACS_AddressChangeTermsInvoice_Test"></class>
		</classes>
	</test>
	
	<test name="ACS Address Validation Test" group-by-instances="true">
		<classes>
			<class name="com.qait.tests.ACS_Address_Validation_Test" />
		</classes>
	</test>
	
	<test name="ACS MYD Transfer" group-by-instances="true">
		<classes>
			<class name="com.qait.tests.ACS_MYD_Transfer"></class>
		</classes>
	</test>
	
	<test name="ACS_Batch_Processing">
		<classes>
			<class name="com.qait.tests.ACS_Batch_Processing"></class>
		</classes>
	</test>
	
	<test name="ACS Reports" group-by-instances="true">
		<classes>
			<class name="com.qait.tests.ACS_Reports"></class>
		</classes>
	</test>
	
	<test name="ACS BPA 1 and BPA 2" group-by-instances="true">
		<classes>
			<class name="com.qait.tests.ACS_BPA_Test" />
		</classes>
	</test>
	
	<test name="ACS Chapter Relationships Test" group-by-instances="true">
		<classes>
			<class name="com.qait.tests.ACS_Chapter_Relationships_Test"></class>
		</classes>
	</test>

	<test name="ACS Email Communication Preferences Test">
		<classes>
			<class name="com.qait.tests.ACS_Email_Communication_Preferences_Test" />
		</classes>
	</test>

	<test name="ACS Return Cancel Refund" thread-count="10" parallel="instances">
		<classes>
			<class name="com.qait.tests.ACS_ReturnCancelRefund" />
		</classes>
	</test>
	
	<test name="ACS Void Invoice Test">
		<classes>
			<class name="com.qait.tests.ACS_Void_Invoice_Test" />
		</classes>
	</test>
	
	<test name="Fundraising Module" group-by-instances="true">
		<classes>
			<class name="com.qait.tests.ACS_Fundraising_Module_Test" />
		</classes>
	</test>
	
	<test name="Membership Renewal Test" group-by-instances="true">
		<classes>
			<class name="com.qait.tests.Membership_Renewal_Test"></class>
		</classes>
	</test>

</suite>