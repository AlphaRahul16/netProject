		</classes>
	</test>
	<test name="ACS Void Invoice Test">
		<classes>
			<class name="com.qait.tests.ACS_Void_Invoice_Test" />
		</classes>
	</test>

	<test name="ACS Fellows Nomination Smoke Test" group-by-instances="true">
		<classes>
			<class name="com.qait.tests.ACS_FellowNominate" />
		</classes>
	</test>

	<test name="ACS Member Transfer Smoke Test" group-by-instances="true">
		<classes>
			<class name="com.qait.tests.ACS_Member_Transfer_Smoke_Test"></class>
		</classes>
