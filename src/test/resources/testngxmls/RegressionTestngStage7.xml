			<class name="com.qait.tests.ACS_Reports"></class>
		</classes>
	</test>

	<test name="ACS_Apply_Payment_Test">
		<classes>
			<class name="com.qait.tests.ACS_Apply_Payment_Test"></class>
		</classes>
	</test>

